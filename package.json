{
  "name": "@karinjs/prebuild-install",
  "version": "1.3.7",
  "description": "A command line tool to easily install prebuilt binaries for multiple version of node/iojs on a specific platform",
  "scripts": {
    "test": "tape test/*-test.js",
    "hallmark": "hallmark --fix --ignore CHANGELOG.md",
    "build": "vite build --config vite.config.rc.ts && vite build"
  },
  "keywords": [
    "prebuilt",
    "binaries",
    "native",
    "addon",
    "module",
    "c",
    "c++",
    "bindings",
    "devops",
    "napi"
  ],
  "dependencies": {
    "detect-libc": "^2.0.0",
    "expand-template": "^2.0.3",
    "github-from-package": "0.0.0",
    "minimist": "^1.2.3",
    "mkdirp-classic": "^0.5.3",
    "napi-build-utils": "^2.0.0",
    "node-abi": "^3.3.0",
    "pump": "^3.0.0",
    "rc": "^1.2.7",
    "simple-get": "^4.0.0",
    "tar-fs": "^2.0.0",
    "tunnel-agent": "^0.6.0"
  },
  "devDependencies": {
    "@types/node": "^18.19.115",
    "a-native-module": "^1.0.0",
    "hallmark": "^4.0.0",
    "nock": "^10.0.6",
    "rimraf": "^2.5.2",
    "standard": "^16.0.4",
    "tape": "^5.3.1",
    "tempy": "0.2.1",
    "terser": "^5.43.1",
    "vite": "^6.3.5"
  },
  "bin": {
    "prebuild-install": "./prebuild-install.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/karinjs/prebuild-install.git"
  },
  "author": "Mathias Buus (@mafintosh)",
